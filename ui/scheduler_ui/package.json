    "@types/node": "20.19.11",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.34.0",
    "eslint-plugin-vue": "9.33.0",
    "globals": "15.15.0",
    "prettier": "3.6.2",
    "typescript": "5.5.4",
    "vite-plugin-checker": "0.9.3",
